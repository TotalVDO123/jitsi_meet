{
    "include": ["react/features/**/*.ts", "react/features/chat/components/web/ShowUnreadMessagesButton.js"],
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "module": "es6",
        "target": "es6",
        "jsx": "react",
        "lib": [ "webworker", "ES2020", "DOM" ],
        "noEmit": false,
        "moduleResolution": "Node",
        "strict": true,
        "noImplicitAny": true,
        "baseUrl": "src/",
    },
    "exclude": [
        "node_modules"
    ]
}
